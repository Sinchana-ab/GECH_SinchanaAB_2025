package com.inheritance.crud.dto;

public class UserDTO {
	
	private String name;
	private String emial;
	private int phone;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmial() {
		return emial;
	}
	public void setEmial(String emial) {
		this.emial = emial;
	}
	public int getPhone() {
		return phone;
	}
	public void setPhone(int phone) {
		this.phone = phone;
	}
	public UserDTO(String name, String emial, int phone) {
		super();
		this.name = name;
		this.emial = emial;
		this.phone = phone;
	}
	public UserDTO() {
		super();
	}
	
}
